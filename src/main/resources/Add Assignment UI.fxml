<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="560.0" prefWidth="483.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.studybuddy.AddAssignmentController">
   <children>
      <Label text="New Assignment" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
      <TextArea fx:id="assignmentName" prefHeight="113.0" prefWidth="437.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Due" />
      <TextArea fx:id="dueDate" prefHeight="138.0" prefWidth="436.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time" />
      <TextArea fx:id="predictedTime" prefHeight="119.0" prefWidth="436.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Description" />
      <TextArea fx:id="assignmentDescription" prefHeight="125.0" prefWidth="436.0" />
      <Button fx:id="addAssignmentButton" mnemonicParsing="false" onAction="#addAssignment" text="Add" />
   </children>
</VBox>
